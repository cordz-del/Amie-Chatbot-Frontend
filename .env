# Application Configuration
VITE_APP_NAME=Amie Chatbot
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=production

# API Security
VITE_API_TIMEOUT=30000
VITE_MAX_RETRIES=3
VITE_RATE_LIMIT=100

# Backend Configuration
VITE_BACKEND_URL=https://amie-chatbot-backend-raarongraham.replit.app
VITE_API_VERSION=v1
VITE_WEBSOCKET_URL=wss://amie-chatbot-backend-raarongraham.replit.app/ws

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_OPENAI_API_ENDPOINT=https://api.openai.com/v1/chat/completions
VITE_OPENAI_MODEL=gpt-3.5-turbo
VITE_OPENAI_MAX_TOKENS=2048
VITE_OPENAI_TEMPERATURE=0.7
VITE_WHISPER_API_ENDPOINT=https://api.openai.com/v1/audio/transcriptions
VITE_WHISPER_MODEL=whisper-1

# ElevenLabs Configuration
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key
VITE_ELEVENLABS_VOICE_ID=your_voice_id
VITE_ELEVENLABS_API_ENDPOINT=https://api.elevenlabs.io/v1/text-to-speech
VITE_ELEVENLABS_MODEL=eleven_monolingual_v1
VITE_ELEVENLABS_STABILITY=0.5
VITE_ELEVENLABS_SIMILARITY=0.75

# Feature Flags (Production)
VITE_ENABLE_VOICE=true
VITE_ENABLE_CHAT_HISTORY=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# Analytics & Monitoring (Production)
VITE_LOG_LEVEL=error
VITE_ENABLE_DEVTOOLS=false
VITE_DEBUG_MODE=false
VITE_MOCK_API=false

# Cache Configuration
VITE_CACHE_TTL=3600
VITE_MAX_CACHE_SIZE=100

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_DEFAULT_LANGUAGE=en
VITE_MAX_MESSAGE_LENGTH=1000
VITE_TYPING_SPEED=50
